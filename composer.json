{
    "name": "daresheep/kubevirt-client-php",
    "description": "kubevirt client for php",
    "keywords": [
        "kubevirt",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://kubevirt.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "daresheep",
            "homepage": "https://github.com/daresheep/kubevirt-client-php"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
